#ifndef NOTIFY_KERNEL_PRIVATECONFIG_H
#define NOTIFY_KERNEL_PRIVATECONFIG_H

// Define to 2 if the system does not provide POSIX.1 features except with
// this defined.
#cmakedefine NOTIFY_POSIX_1_SOURCE 1

// Define to 1 if you need to in order for `stat' and other things to work.
#cmakedefine NOTIFY_POSIX_SOURCE 1

// Enable GNU extensions on systems that have them.
#cmakedefine NOTIFY_GNU_SOURCE 1

// header detect result
#cmakedefine NOTIFY_HAVE_ENDIAN_H

#endif //NOTIFY_KERNEL_PRIVATECONFIG_H
